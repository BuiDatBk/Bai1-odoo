<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_partner_form_inherit" model="ir.ui.view">
            <field name="name">advanced_sale.res_partner.form</field>
            <field name="model">ProjectName.res_partner</field>
            <field name="inherit_id" ref="ProjectName.ViewExternalID"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='example_field']" position="replace">
                    <field name="example_field2"/>
                </xpath>
                <xpath expr="//field[@name='example_field']" position="attributes">
                    <attribute name="invisible">True</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>